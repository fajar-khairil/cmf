        "illuminate/cache": "@stable",
        "illuminate/redis": "@stable",
        "silex/web-profiler": "*",
        "illuminate/filesystem": "@stable",
        "illuminate/config": "@stable",
        "brandonwamboldt/utilphp": "1.0.6",
